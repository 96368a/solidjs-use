      "optional": true
    },
    "universal-cookie": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.5.0",
    "async-validator": "^4.2.5",
    "axios": "^1.1.3",
    "change-case": "^4.1.2",
    "drauu": "^0.3.2",
