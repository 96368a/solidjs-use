      "types": "./dist/useNProgress.d.ts",
      "require": "./dist/useNProgress.cjs",
      "import": "./dist/useNProgress.mjs"
    },
    "./useQRCode": {
      "types": "./dist/useQRCode.d.ts",
      "require": "./dist/useQRCode.cjs",
      "import": "./dist/useQRCode.mjs"
    },
    "./useChangeCase": {
      "types": "./dist/useChangeCase.d.ts",
      "require": "./dist/useChangeCase.cjs",
      "import": "./dist/useChangeCase.mjs"
    },
    "./useAsyncValidator/component": {
      "types": "./dist/useAsyncValidator/component.d.ts",
      "require": "./dist/useAsyncValidator/component.cjs",
      "import": "./dist/useAsyncValidator/component.mjs"
    },
    "./useIDBKeyval": {
      "types": "./dist/useIDBKeyval.d.ts",
      "require": "./dist/useIDBKeyval.cjs",
      "import": "./dist/useIDBKeyval.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.mjs",
  "jsdelivr": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "solidjs-use": "workspace:*"
  },
  "peerDependencies": {
    "async-validator": "*",
    "axios": "*",
    "change-case": "*",
    "drauu": "*",
    "focus-trap": "*",
